{
    "name": "pderas/laravel-code-scans",
    "description": "Reusable code scan rules to mimic the abandoned Enlightn scanning tool.",
    "type": "library",
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^9.0|^10.0|^11.0|^12",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "larastan/larastan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Pderas\\LaravelCodeScans\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pderas\\LaravelCodeScans\\LaravelCodeScansServiceProvider"
            ]
        }
    }
}
